w = [30, 20, 15]
c = 35

def greed(w,c):
    wc = 0                      //загрузка
    for i in range(len(w)):
        if w[i] > c:            //если вес > вместимости, то уходим
            continue            // пропускает последующие операции в цикле , если условие верно
        wc += w[i]              //загрузка + вес
        c -= w[i]               //вместимость - вес
    return wc                   //загрузку из функции

wc = greed(w, c)
print(wc)                       //"жадная" загрузка
print("cena: " + str(wc*100))   //"жадная" цена
