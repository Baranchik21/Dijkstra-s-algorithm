#include <iostream> // подключение библиотеки, для возможности вывода в консоль
using namespace std; // указание пространства имён, для кода ниже
void main()
{
	setlocale(LC_ALL, "Rus"); //делает расшифровку для русских символов
	int a = 0,f = 1,n,p; 
	int d;    //Счётчик считывает количество делений
	cin >> n;
	while (a < n) {
		a++;
		f *= a;
	}
	cout <<"Факториал числа "<<n<<": "  <<f<<endl;

	cout << "Простые числа до "<< n<< ": ";
	for (int i = 0; i < n; ++i) { // цикл от 0 до n
		for (int j = 1; j < n; ++j) { 
			
			if (i % j == 0) {   // если остаток от деления равен 0
				d++;   // увеличиваем счётчик
			}
		}
		if (d < 3) {    // если у числа только два делителя
			p = i;    //Присваеваем это числов результат
			cout  << p << " ";    //выводим результат
		}
		d = 0;  //обнуляем счётчик
	}
}
